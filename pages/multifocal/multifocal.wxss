/* pages/multifocal/multifocal.wxss */
page{
  background: black;
  height: 100vh;
}
.img{
  width: 100%;
  height: 48vh;
}
.button_container{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 7vh;
}
/* 按钮未选中 */
.normal_button{
  background: black;
  color: white;
}
/* 按钮选中 */
.checked_button{
  background: white;
  color: black;
  border-radius: 50%;
}